embed <drac2>
A = &ARGS&
a = argparse(A)
ch = character()

baseCollect = load_yaml(get_gvar('5cc7f466-d74a-465f-b8e9-68dcf87240ef'))

collectList = []
refineList = []
for x in baseCollect:
  if x['atype'] == 'collect':
    collectList += x
  if x['atype'] == 'refine':
    refineList += x

if a.get('collect'): 
  pArgs = []
  for x in collectList:
    err(dump_json(x))
    pArgs += ' '.split(x['action'])
    err(pArgs)

return f''' -title "NO" '''
</drac2>